# MeroDocs Application Configuration
# Copy this file to .env and update with your actual values

# Calimero Application ID
VITE_APPLICATION_ID="your_application_id_here"

# NEAR Network Environment (testnet, mainnet, etc.)
VITE_NEAR_ENVIRONMENT=testnet

# Node URL for the application
VITE_NODE_URL=http://localhost:2428

# RPC endpoint path
VITE_RPC_PATH=/jsonrpc

# Context ID for the application
VITE_CONTEXT_ID="your_context_id_here"
