# Calimero Application ID
VITE_APPLICATION_ID="your-application-id-here"

# Calimero Application Path (WASM file)
VITE_APPLICATION_PATH="https://your-wasm-path-here.wasm"

# DFX Network Configuration
# Set to 'ic' for mainnet, 'local' for local development
VITE_DFX_NETWORK=ic

# ICP Backend Canister ID
# Replace with your deployed canister ID
VITE_BACKEND_CANISTER_ID=your-backend-canister-id-here

# Internet Identity Canister ID
# For mainnet (official Internet Identity)
VITE_INTERNET_IDENTITY_CANISTER_ID=rdmx6-jaaaa-aaaah-qdrqq-cai

# For local development, use these instead:
# VITE_DFX_NETWORK=local
# VITE_BACKEND_CANISTER_ID=your-local-canister-id
# VITE_INTERNET_IDENTITY_CANISTER_ID=be2us-64aaa-aaaaa-qaabq-cai